<form action="<%=@action%>" method="post">
          <%if @method%>
            <input type="hidden" name="_method" value="<%=@method%>"/>
          <%end%>
<a href="/contacts/new">new!</a>

<table>
  <tr>
    <%@headers[0..3].each do |title| %>
      <th><%=title%></th>
    <%end%>
    <th></th>
    <th></th>
  </tr>
  <%@contacts.each do |contact|%>
    <tr>
      <td><a href="/contacts/<%=contact['id']%>"><%=contact['id']%></td>
      <%@headers[1..3].each do |key| %>
        <td><%=contact[key]%></td>
      <%end%>
      <td><a href="/contacts/<%=contact['id']%>/edit">edit</a></td>
      <td>
        <a href="/contacts/<%=contact['id']%>/destroy" >delete</a>
      </td>
    </tr>
  <%end%>

</table>
</form>